{
      "version": 2,
      "builds": [
            {
                  "src": "aluno/wsgi.py",
                  "use": "@vercel/python",
                  "config": {
                        "maxLambdaSize": "15mb",
                        "runtime": "python3.14.0"
                  }
            }
      ],
      "routes": [
            {
                  "src": "/static/(.*)",
                  "dest": "/static/$1"
            },
            {
                  "src": "/(.*)",
                  "dest": "aluno/wsgi.py"
            }
      ]
}